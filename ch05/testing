from ssl import ALERT_DESCRIPTION_UNEXPECTED_MESSAGE


def square(num):
    result = 0
    for i in range(abs(num)):
        result += abs(num)
    return result

def main():
    res = square(5)
    print(res)



def removeVowels(text):
    vowels = "aeiou"
    vowels += vowels.upper()
    newText = ""
    for ch in text:
        if ch not in vowels:
            newText += ch
    return newText

def main():
    mystr = "compsci"
    print(removeVowels(mystr))
    mystr = "LEEEEEEEROY JENKINS!"
    print(removeVowels(mystr))
main()
